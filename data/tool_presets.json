    },
    "stainless_steel": {
      "recommended_coatings": ["TiAlN", "AlCrN", "DLC"],
      "avoid_coatings": ["uncoated"],
      "flute_recommendations": {
        "roughing": [2, 3],
        "finishing": [4]
      },
      "speed_multiplier": 0.4,
      "notes": "Work hardens rapidly. Maintain consistent feed rate and use sharp tools."
    },
    "titanium": {
      "recommended_coatings": ["TiAlN", "AlCrN"],
      "avoid_coatings": ["TiN", "uncoated"],
      "flute_recommendations": {
        "roughing": [2],
        "finishing": [3, 4]
      },
      "speed_multiplier": 0.3,
      "notes": "Very challenging material. Low speeds, high feeds, flood coolant essential."
    }
  },
  "application_guides": {
    "3d_profiling": {
      "recommended_types": ["ball_endmill", "corner_radius_endmill"],
      "flute_count": [2, 3, 4],
      "notes": "Ball end mills for complex 3D surfaces. Consider corner radius for blend of strength and finish."
    },
    "roughing": {
      "recommended_types": ["roughing_endmill", "square_endmill"],
      "flute_count": [2, 3],
      "notes": "Roughing end mills for high material removal rates. Lower flute count for better chip evacuation."
    },
    "finishing": {
      "recommended_types": ["square_endmill", "ball_endmill"],
      "flute_count": [4, 6],
      "notes": "Higher flute count for better surface finish. Consider tool deflection for deep cuts."
    },
    "slotting": {
      "recommended_types": ["square_endmill"],
      "flute_count": [2, 3],
      "notes": "Lower flute count for better chip evacuation in enclosed slots."
    },
    "pocketing": {
      "recommended_types": ["square_endmill", "corner_radius_endmill"],
      "flute_count": [3, 4],
      "notes": "Adaptive toolpaths recommended. Corner radius end mills reduce stress concentrations."
    }
  },
